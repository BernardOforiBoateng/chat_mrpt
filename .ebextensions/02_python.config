option_settings:
  aws:elasticbeanstalk:application:environment:
    PYTHONPATH: /opt/python/current/app
    FLASK_APP: wsgi.py
    FLASK_ENV: production
  aws:elasticbeanstalk:container:python:
    WSGIPath: wsgi.py
    NumProcesses: 2
    NumThreads: 15
  aws:elasticbeanstalk:container:python:staticfiles:
    /static/: app/static/